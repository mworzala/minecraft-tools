#!/usr/bin/env python3

import sys

def main():
    version = None
    if len(sys.argv) > 1:
        version = sys.argv[1]
    else: version = determine_latest_version()
    
    print(f"Downloading tools for {version}...")

    # if (len(sys.argv) == 1)
    return 0

def determine_latest_version():
    return "1.18.1"

if __name__ == '__main__':
    sys.exit(main())
